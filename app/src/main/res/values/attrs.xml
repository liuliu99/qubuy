    </declare-styleable>

    <declare-styleable name="gridPasswordView">
        <attr name="textColorPayInput" format="color|reference" />
        <attr name="textSizePayInput" format="dimension" />
        <attr name="lineColorPayInput" format="color" />
        <attr name="gridColorPayInput" format="color" />
        <attr name="lineWidthPayInput" format="dimension" />
        <attr name="passwordLengthPayInput" format="integer" />
        <attr name="passwordTransformationPayInput" format="string" />
        <attr name="passwordTypePayInput" format="enum">
            <enum name="numberPassword" value="0" />
            <enum name="textPassword" value="1" />
            <enum name="textVisiblePassword" value="2" />
            <enum name="textWebPassword" value="3" />
        </attr>
    </declare-styleable>
